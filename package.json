{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "npx eslint . --format=stylish",
    "lint-fix": "npx eslint . --format=stylish --fix",
    "prettier": "prettier --check \"**/{*.vue,*.js,*.ts,*.html,*.css,*.scss,*.json,*.mdx,*.md}\"",
    "prettier-fix": "prettier --write \"**/{*.vue,*.js,*.ts,*.html,*.css,*.scss,*.json,*.mdx,*.md}\"",
    "generate-api-types": "openapi-generator-cli  generate -i openapi.bundled.yaml -g typescript-angular -o ./types/api/.",
    "prod:prisma:migrate:deploy": "npx prisma migrate deploy",
    "deploy-start": "npm run prod:prisma:migrate:deploy && node ./.output/server/index.mjs",
    "prisma:generate": "prisma generate",
    "setup": "npm ci && prisma generate && npm run build",
    "prod:build": "npm run prisma:generate && npm run build",
    "fill-db": "node prisma/fill-db.mjs"
  },
  "author": "Yves Hary <yves.hary@ergosign.de>",
  "devDependencies": {
    "@invictus.codes/nuxt-vuetify": "^0.3.0",
    "@nuxt/devtools": "latest",
    "@nuxt/types": "^2.17.3",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@openapitools/openapi-generator-cli": "^2.9.0",
    "@prisma/client": "^5.6.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "nuxt": "^3.10.0",
    "nuxt-mdi": "^1.1.1",
    "prettier": "^3.1.0",
    "prisma": "^5.6.0",
    "sass": "^1.69.5",
    "typescript": "^5.3.2",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.5.1",
    "pdf-text-reader": "4.0.1",
    "pinia": "^2.1.7",
    "vue-material-design-icons": "^5.2.0"
  }
}
